import sun.security.util.Length

fun accessible(gridInput: String): Int {
    val gridSplit = gridInput.split("\n")
    val length = gridSplit[0].length
    val grid = gridSplit.joinToString("")

    var count = 0

    grid.forEachIndexed { index, pos ->
        val adjacentValues = mutableListOf<String?>()
        repeat(3) {
            if (isAtLeftEdge(index, length) && it == 0) {
                adjacentValues.addAll(listOf(null, null, null))
            } else if (isAtRightEdge(index, length) && it == 2) {
                adjacentValues.addAll(listOf(null, null, null))
            } else {
                grid.getOrNull(index + length - 1 + it).toString()
                adjacentValues.add(
                    grid.getOrNull(index - length - 1 + it).toString()
                )
                if (it != 1) {
                    adjacentValues.add(
                        grid.getOrNull(index - 1 + it).toString()
                    )
                }
                adjacentValues.add(
                    grid.getOrNull(index + length - 1 + it).toString()
                )
            }
        }
        if (adjacentValues.count { it == "@" } < 4 && grid.getOrNull(index).toString() == "@") {
            count++
        }
    }

    return count
}

fun isAtLeftEdge(index: Int, length: Int): Boolean {
    return index % length == 0
}

fun isAtRightEdge(index: Int, length: Int): Boolean {
    return (index - length - 1) % length == 0
}

val sample = """
    ..@@.@@@@.
    @@@.@.@.@@
    @@@@@.@.@@
    @.@@@@..@.
    @@.@@@@.@@
    .@@@@@@@.@
    .@.@.@.@@@
    @.@@@.@@@@
    .@@@@@@@@.
    @.@.@@@.@.
""".trimIndent()

